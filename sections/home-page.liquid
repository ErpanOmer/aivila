<script type="module">
    let isMobile = false // 默认为false

    // 监听页面视口宽度，小于767时隐藏所有PC类，反之隐藏所有MOB类
    function handleViewportClass() {
        isMobile = window.innerWidth < 767;
        const pcElements = document.querySelectorAll('.PC');
        const mobElements = document.querySelectorAll('.MOB');
        pcElements.forEach(el => {
            el.style.display = isMobile ? 'none' : '';
        });
        mobElements.forEach(el => {
            el.style.display = isMobile ? '' : 'none';
        });
        console.log('是否是移动端：', isMobile);

        // 动态加载js，且避免重复引入
        // 判断页面是否已经引入了对应的js，如果已引入则不再引入
        const scriptId = 'aivela_dynamic';
        const targetSrc = isMobile ? "{{ 'aivela_mob.js' | asset_url }}" : "{{ 'aivela_pc.js' | asset_url }}";
        const existingScript = document.getElementById(scriptId);

        // 检查是否已存在且src一致
        if (existingScript) {
            // 如果已经是目标脚本，则不做任何操作
            if (existingScript.getAttribute('src') === targetSrc) {
                // 已经加载了目标脚本，无需重复加载
                return;
            } else {
                // src不一致，移除旧的，准备加载新的
                existingScript.parentNode.removeChild(existingScript);
            }
        }

        // 创建并加载新的脚本，采用“立即执行函数”包裹，避免变量污染全局作用域
        const script = document.createElement('script');
        script.id = scriptId;
        script.src = targetSrc;
        // 通过设置type为module，避免变量重复声明报错
        script.type = 'module';
        document.body.appendChild(script);

    }

    // 初始执行一次
    handleViewportClass();
    // 监听窗口大小变化
    window.addEventListener('resize', handleViewportClass);

</script>
